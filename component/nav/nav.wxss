.mod_aside {
  position: fixed;
  bottom: 180rpx;
  right: -520rpx;
  transform: translateX(0);
  transition: transform 0.4s ease;
}

.mod_aside.open {
  transform: translateX(-520rpx);
}

.aside_nav {
  width: 520rpx;
  padding: 20rpx 0 20rpx;
  background: #fff;
  border-radius: 8rpx 0 0 8rpx;
  height: 184rpx;
  position: relative;
  /* bottom: 180rpx;
  right: 0; */
  overflow: hidden;
  z-index: 99;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
}

.aside_nav .aside_nav_item {
  width: 130rpx;
  height: 104rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
  float: left;
  text-align: center;
  font-size: 20rpx;
  line-height: 36rpx;
}

.aside_nav .aside_nav_item .aside_icon {
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}
