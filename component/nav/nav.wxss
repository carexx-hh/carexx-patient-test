.nav_btn {
  width: 90rpx;
  height: 80rpx;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  left: -90rpx;
  bottom: 100rpx;
  border-radius: 8rpx 0 0 8rpx;
  display: flex;
}
.nav_btn .btn_icon{
  width: 40rpx;
  height: 40rpx;
  margin: auto 0;
  background: url(data:image/png;base64,) no-repeat center center/40rpx auto;
}

.nav_btn text {
  width: 50rpx;
  color: #fff;
  font-size: 22rpx;
  line-height: 25rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin: auto 0;
}


